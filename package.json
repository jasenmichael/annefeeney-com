{
  "name": "nuxt-supercharged-boilderplate",
  "description": "Features include: Netlify Identity, NetlifyCms, PWA, TWA, Nuxt Content, SEO meta in head, flat-file site settings, and more",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "oldDev": "npm run tumblrCache && nuxt",
    "dev": "nuxt",
    "oldBuild": "npm run tumblrCache && nuxt build",
    "build": "nuxt build",
    "start": "nuxt start",
    "oldGenerate": "npm run tumblrCache && nuxt generate",
    "generate": "nuxt generate",
    "tumblrCache": "node -e \"require('./plugins/tumblr').cacheTumblrDownloadImages('annefeeney')\"",
    "cmsProxy": "npx netlify-cms-proxy-server"
  },
  "dependencies": {
    "@mdi/js": "^5.8.55",
    "@nuxt/content": "^1.9.0",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/ngrok": "^2.0.0",
    "@nuxtjs/pwa": "^3.3.2",
    "core-js": "^3.6.5",
    "load-json-file": "^6.2.0",
    "netlify-plugin-submit-sitemap": "^0.2.3",
    "nuxt": "^2.14.9",
    "vue-masonry": "^0.12.0",
    "write-json-file": "^4.3.0"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^3.4.2",
    "@nuxtjs/vuetify": "^1.11.2",
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "autoprefixer": "^9.8.6",
    "nuxt-font-loader": "^1.1.2",
    "nuxt-purgecss": "^1.0.0",
    "postcss": "^7.0.35",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.3"
  }
}
