{
  "name": "nuxt-supercharged-boilderplate",
  "description": "Features include: Netlify Identity, NetlifyCms, PWA, TWA, Nuxt Content, SEO meta in head, flat-file site settings, and more",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run tumblrCache && nuxt",
    "build": "npm run tumblrCache && nuxt build",
    "start": "nuxt start",
    "generate": "npm run tumblrCache && nuxt generate",
    "tumblrCache": "node -e \"require('./plugins/tumblr').cacheTumblrDownloadImages('annefeeney')\""
  },
  "dependencies": {
    "@mdi/js": "^5.8.55",
    "@nuxt/content": "^1.9.0",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/ngrok": "^2.0.0",
    "@nuxtjs/pwa": "^3.3.2",
    "core-js": "^3.6.5",
    "load-json-file": "^6.2.0",
    "nuxt": "^2.14.9",
    "vue-masonry": "^0.12.0",
    "write-json-file": "^4.3.0"
  },
  "devDependencies": {
    "@nuxtjs/vuetify": "^1.11.2",
    "nuxt-font-loader": "^1.1.2",
    "nuxt-purgecss": "^1.0.0"
  }
}
